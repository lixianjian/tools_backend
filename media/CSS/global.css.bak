@charset "utf-8";
/* CSS Document */
* {margin:0px auto;; padding:0px;}/* border:0px; outline:0px;}*/
*a {text-decoration:none;-moz-outline:none;}
a:hover {color:#FC0; text-decoration:underline;}
ul,li,dl,dt,dd,ol {list-style-type:none; display:block;}
h1,h2,h3,h4,h5 {font-size:14px; display:block;}
p,form {display:block;}
.ovhidden {overflow:hidden;}
body {
	font-family: "Trebuchet MS", Arial, "宋体";
	font-size:14px;
	margin:0px;
	background:#F3F3F3;
	color:#333;
}
#container {
	position:relative;
	width:100%;
	margin:0px auto;
}
#header {
	width:960px;
	/*float:left;*/
	clear:both;
	background-color:#FFF;
}
#banner {
	width:941px;
	height:58px;
	float:left;
	clear:both;
	margin-left:10px;
	background:url(../images/banner1.png) no-repeat;
}
#logo {
	width:200px;
	height:58px;
	float:none;
	margin-top:auto;
	margin-left:370px;
	overflow:hidden;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 32px;
	font-style: normal;
	text-decoration: none;
	line-height: 58px;
	text-align: center;
	color: #000;
	background-color: #EEE;
}
#logo a {
	cursor:pointer;
	text-decoration: none;
	color: #000;
}
#logo a:hover {
	color: #000;
	text-decoration: none;
}

#nav {
	width:950px;
	height:40px;
	float:left;
	clear:both;
	background:#333;
	padding-left:10px;
	overflow:hidden;
}
#nav li {
	width:120px;
	height:40px;
	padding:0px 11px 0px 10px;
	float:left;
	background:url(../images/nav_li_bg.png) right center no-repeat #333;
	line-height:40px;
	text-align:center;
	font-size: 20px;
}
#nav li.infotxt {
	width:245px;
	padding:0px;
	line-height:40px;
	background:#333;
	color:#999;
}
#nav li a {
	display:block;
	width:120px;
	height:40px;
	float:left;
	clear:both;
	font-weight:bold;
	color:#FFF;
}
#nav li a:hover {
	color:#FC0;
	text-decoration:none;
	border:0px;
	background:url(../images/nav_a_hover_bg.png) no-repeat;
}
#nav li a.this {
	background:url(../images/nav_a_this_bg.png) bottom no-repeat #333;
	cursor:default;
}
#nav li a.this:hover {
	background:url(../images/nav_a_this_bg.png) bottom no-repeat #333;
	color:#FFF;
}
#navshadow {
	width:960px;
	height:10px;
	float:left;
	clear:both;
	background:url(../images/navshadow_bg.png) repeat-x;
}
#content {
	width:1400px;
	/*float:left;*/
	clear:both;
	padding-bottom:20px;
}
#middle {
	width:1440px;
	clear:both;
	text-align:center;
	line-height:30px;
	color:#666;
}
#leftsider {
	width:250px;
	float:left;
}
#leftsider h1 {
	width:232px;
	height:40px;
	float:left;
	clear:both;
	padding:5px 0px 1px 18px;
	background:url(../images/sidebar_title.jpg) no-repeat;
	color:#333;
	line-height:40px;
}

#leftsider .boxcont {
	width:230px;
	float:left;
	clear:both;
	line-height:24px;
	background:url(../images/sidebar_content_bg.png) repeat-y;
	padding:5px 10px;
}

#leftsider .bottom {
	width:250px;
	height:15px;
	float:left;
	clear:both;
	background:url(../images/sidebar_bottom.png) no-repeat;
	overflow:hidden;
}

.function{
	width:206px;
	line-height:24px;
	padding-left:24px;
	float:left;
	clear:both;
	background:url(../images/Article_li_img.png) left no-repeat;
	color:#999;
	background-color: #EEE;
	border-bottom: 1px;
	border-bottom-style: solid;
	border-bottom-color: #CCC;
	position: relative;
	font-size: 18px;	
	color: #000;
}
.function a {
	color: #000;
	text-decoration: none;
}

.function a:hover {
	color: #F00;	
}

.type{
	width:206px;
	line-height:24px;
	padding-left:24px;
	float:left;
	clear:both;
	/*background:url(../images/Article_li_img.png) left no-repeat;*/
	color:#999;
	background-color: #EEE;
	border-bottom: 1px;
	border-bottom-style: solid;
	border-bottom-color: #CCC;
	position: relative;
	font-size: 14px;	
	color: #000;
}
.type a {
	color: #000;
	text-decoration: none;
}

.type a:hover {
	color: #F00;
}

#rightsider {
	background-color: #FFF;
	float: right;
	width: 1130px;
	border-left:solid #CCC 5px;
	margin: auto;
	padding-left: 5px;
}

#search	{
	margin: auto;
	border-bottom:solid #CCC 5px;
	margin-bottom:10px;
	padding-bottom:5px;
}

#search .s_name {
	border:1px solid #000;	
}

#search .date{
	width:80px;
	height:20px;
	background-color:#CCC;
	border:1px solid #000;
}

#search .btn{
	width:70px;
	height:25px;
	font-size:14px; 
	font-weight:bold;
	cursor:pointer;
}

form	{
	line-height:25px;
}

form hr {
	margin-top:5px;
	margin-bottom:5px;
}

form fieldset {
	width:650px;
}

form .intro {
	width:100px;
	float:left;
}

#rightsider .data_title {
	margin-bottom:10px;
	width:100%;
	height:30px;
	float:left;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	font-style: normal;
	text-decoration: none;
	line-height:30px;
	color: #000;
	background-color: #EEE;
	border-bottom:solid #000 1px;
}


#rightsider .data{
	width: 100%;
	border:solid #000 1px;
	margin: auto;
	border-collapse:collapse;
	margin-bottom:10px;
}


#rightsider .data th{
	background-color:#CCC;
	float:inherit;
	font-size: 14px;
	font:"宋体" #FFF;
	border:1px solid #000;
}

#rightsider .data td{
	border:1px solid #000;
	text-align:center;
}

#placeholder {
	 width:600px;
	 height:300px;
	 float:left;
	 border:solid #000 1px;	
	 margin: auto;
}

#overviewLegend {
	 width:85px;
	 float:right;
	 border:solid #000 1px;
	 margin: auto;
}

#footer {
	width:960px;
	height:100px;
	/*float:left;*/
	clear:both;
	border-top:5px solid #CCC;
	text-align:center;
	line-height:30px;
	color:#666;
}

/* 表单 */
#form_table .regist_table{
	width: 100%;
	border:solid #FFF 1px;
	margin: auto;
	border-collapse:collapse;
	margin-bottom:10px;
	background-color:#EEE;
}

#form_table .right_td{
	border:1px solid #FFF;
	text-align:right;
	width:100px;
	height:30px;
	font-size: 14px;
	font:"宋体" #FFF;
}
#form_table .left_td{
	border:1px solid #FFF;
	text-align:left;
	width:250px;
	height:30px;
}

#form_table .btn{
	font-weight:bold;
	font-size:14px;
	padding:2px 5px 2px 5px;
	}

.submit_btn {
	font-weight:bold;
	font-size:14px;
	padding:2px 5px 2px 5px;
	}